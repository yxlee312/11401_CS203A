#include <stdio.h>
#include <stdlib.h>
int main() 
{
    int *array;
    int n = 10;
    array = (int *) malloc(n * sizeof(int));
    if (array == NULL) 
    {
        return 1;
    }
    for (int i = 0; i < n; i++)  
    {
        array[i] = i + 1;
    }
    for (int i = 0; i < n; i++) 
    {
        printf("%d ", array[i]);
    }
    printf("\n");
    int old_n = n;
    n = n * 2;
    array = (int *) realloc(array, n * sizeof(int));
    if (array == NULL) 
    {
        return 1;
    }
    for (int i = old_n; i < n; i++) 
    {
        array[i] = i + 1;
    }
    for (int i = 0; i < n; i++) 
    {
        printf("%d ", array[i]);
    }
    printf("\n");
    free(array);
    return 0;
}
